{
    "@id": "urn:seeedkk:ReButton:2",
    "@type": "CapabilityModel",
    "@context": "http://azureiot.com/v1/contexts/IoTModel.json",
    "implements": [
        {
            "name": "DeviceInfo",
            "schema": {
                "@id": "urn:azureiot:DeviceManagement:DeviceInformation:1",
                "@type": "Interface",
                "displayName": "Device Information",
                "contents": [
                    {
                        "@type": "Property",
                        "name": "manufacturer",
                        "displayName": "Manufacturer",
                        "schema": "string",
                        "description": "Company name of the device manufacturer. This could be the same as the name of the original equipment manufacturer (OEM). Ex. Contoso."
                    },
                    {
                        "@type": "Property",
                        "name": "model",
                        "displayName": "Device model",
                        "schema": "string",
                        "description": "Device model name or ID. Ex. Surface Book 2."
                    },
                    {
                        "@type": "Property",
                        "name": "swVersion",
                        "displayName": "Software version",
                        "schema": "string",
                        "description": "Version of the software on your device. This could be the version of your firmware. Ex. 1.3.45"
                    },
                    {
                        "@type": "Property",
                        "name": "osName",
                        "displayName": "Operating system name",
                        "schema": "string",
                        "description": "Name of the operating system on the device. Ex. Windows 10 IoT Core."
                    },
                    {
                        "@type": "Property",
                        "name": "processorArchitecture",
                        "displayName": "Processor architecture",
                        "schema": "string",
                        "description": "Architecture of the processor on the device. Ex. x64 or ARM."
                    },
                    {
                        "@type": "Property",
                        "name": "processorManufacturer",
                        "displayName": "Processor manufacturer",
                        "schema": "string",
                        "description": "Name of the manufacturer of the processor on the device. Ex. Intel."
                    },
                    {
                        "@type": "Property",
                        "name": "totalStorage",
                        "displayName": "Total storage",
                        "schema": "long",
                        "displayUnit": "kilobytes",
                        "description": "Total available storage on the device in kilobytes. Ex. 2048000 kilobytes."
                    },
                    {
                        "@type": "Property",
                        "name": "totalMemory",
                        "displayName": "Total memory",
                        "schema": "long",
                        "displayUnit": "kilobytes",
                        "description": "Total available memory on the device in kilobytes. Ex. 256000 kilobytes."
                    }
                ],
                "@context": "http://azureiot.com/v1/contexts/IoTModel.json"
            }
        },
        {
            "name": "Battery",
            "schema": {
                "@id": "urn:seeedkk:Interface:Battery:1",
                "@type": "Interface",
                "@context": "http://azureiot.com/v1/contexts/IoTModel.json",
                "contents": [
                    {
                        "@type": "Telemetry",
                        "name": "batteryVoltage",
                        "schema": "double",
                        "displayName": {
                            "en": "Battery voltage",
                            "ja": "バッテリー電圧"
                        },
                        "displayUnit": "V"
                    }
                ]
            }
        },
        {
            "name": "PushButton",
            "schema": {
                "@id": "urn:seeedkk:Interface:PushButton:1",
                "@type": "Interface",
                "@context": "http://azureiot.com/v1/contexts/IoTModel.json",
                "contents": [
                    {
                        "@type": [
                            "Telemetry",
                            "SemanticType/State"
                        ],
                        "name": "actionNum",
                        "schema": {
                            "@id": "urn:seeedkk:Interface:PushButton:actionNumType:1",
                            "@type": "Enum",
                            "valueSchema": "integer",
                            "enumValues": [
                                {
                                    "name": "SingleClick",
                                    "enumValue": 1,
                                    "displayName": {
                                        "en": "Single click",
                                        "ja": "シングルクリック"
                                    }
                                },
                                {
                                    "name": "DoubleClick",
                                    "enumValue": 2,
                                    "displayName": {
                                        "en": "Double click",
                                        "ja": "ダブルクリック"
                                    }
                                },
                                {
                                    "name": "TripleClick",
                                    "enumValue": 3,
                                    "displayName": {
                                        "en": "Triple click",
                                        "ja": "トリプルクリック"
                                    }
                                },
                                {
                                    "name": "LongPress",
                                    "enumValue": 10,
                                    "displayName": {
                                        "en": "Long press",
                                        "ja": "ロングクリック"
                                    }
                                },
                                {
                                    "name": "SuperLongPress",
                                    "enumValue": 11,
                                    "displayName": {
                                        "en": "Super long press",
                                        "ja": "スーパーロングクリック"
                                    }
                                }
                            ]
                        },
                        "displayName": {
                            "en": "Action number",
                            "ja": "アクション番号"
                        }
                    },
                    {
                        "@type": [
                            "Telemetry",
                            "SemanticType/Event"
                        ],
                        "name": "message",
                        "schema": "string",
                        "displayName": {
                            "en": "Message",
                            "ja": "メッセージ"
                        }
                    }
                ]
            }
        }
    ],
    "displayName": "ReButton"
}